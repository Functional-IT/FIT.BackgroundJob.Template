{
    "$schema": "http://json.schemastore.org/template",
    "author": "Functional IT",
    "classifications": [
        "Windows Service",
        "Background Job"
    ],
    "name": "FIT Background Job v0.0.2",
    "tags": {
        "language": "F#",
        "type": "project"
    },
    "identity": "FIT.BackgroundJob",
    "shortName": "FIT.Backgroundjob",
    "sourceName": "FIT.BackgroundJob",
    "sources": [
        {
            "source": "./default"
        }
    ],
    "SpecialCustomOperations": {
        "**/paket.*": {
            "operations": [
                {
                    "type": "conditional",
                    "configuration": {
                        "if": [
                            "#if"
                        ],
                        "else": [
                            "#else"
                        ],
                        "elseif": [
                            "#elseif"
                        ],
                        "endif": [
                            "#endif"
                        ],
                        "trim": "true",
                        "wholeLine": "true"
                    }
                }
            ]
        },
        "**/*.fsproj": {
            "operations": [
                {
                    "type": "conditional",
                    "configuration": {
                        "if": [
                            "<!--#if"
                        ],
                        "else": [
                            "<!--#else",
                            "#else-->"
                        ],
                        "endif": [
                            "<!--#endif",
                            "#endif-->"
                        ],
                        "trim": "true",
                        "wholeLine": "true"
                    }
                }
            ]
        },
        "**/*.fs": {
            "operations": [
                {
                    "type": "conditional",
                    "configuration": {
                        "if": [
                            "#if",
                            "//#if",
                            "(*#if"
                        ],
                        "else": [
                            "#else",
                            "//#else",
                            "(*#else",
                            "#else*)"
                        ],
                        "endif": [
                            "#endif",
                            "//#endif",
                            "(*#endif",
                            "#endif*)"
                        ],
                        "trim": "true",
                        "wholeLine": "true"
                    }
                }
            ]
        }
    }
}